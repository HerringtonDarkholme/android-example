package h.chan.compass

class CompassView(ctx: Context,
                  attrs: AttributeSet,
                  style:  Int
  ) extends View(ctx, attrs, style) {
  
  initCompassView()

  def this(ctx: Context) = this(ctx, null, 0)
  def this(ctx: Context, attrs: AttributeSet) =
    this(ctx, attrs, 0)

  def initCompassView: Unit = {
    setFocusa 
  }
}
